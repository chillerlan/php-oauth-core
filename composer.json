{
	"name": "chillerlan/php-oauth-core",
	"description": "A PHP7.2+ OAuth client core library",
	"homepage": "https://github.com/chillerlan/php-oauth-core",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "stable",
	"keywords": [
		"oauth", "oauth1", "oauth2", "client", "core", "psr-7", "psr-17", "psr-18", "rfc5849", "rfc6749"
	],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-oauth-core/issues",
		"source": "https://github.com/chillerlan/php-oauth-core"
	},
	"require": {
		"php": "^7.4",
		"ext-json":"*",
		"ext-simplexml":"*",
		"chillerlan/php-settings-container": "^1.2",
		"chillerlan/php-httpinterface": "^4.0",
		"chillerlan/php-magic-apiclient": "^3.0",
		"psr/http-client":"^1.0",
		"psr/http-message": "^1.0",
		"psr/log": "^1.1"
	},
	"require-dev": {
		"chillerlan/php-dotenv": "^1.0",
		"phpunit/phpunit": "^8.4"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\OAuth\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\OAuthExamples\\": "examples/",
			"chillerlan\\OAuthTest\\": "tests/"
		}
	}
}
